# Test the use of underscores
enum Employee_department {
    MANAGER = "manager"
    LEAD = "lead"
}

ruleset PersonAddress {
    houseNumber int
    street str
    city str
    post_code str
}

ruleset Person {
    first_name str  # Make sure underscores are parsed correctly
    last-name str   #Â Make sure dashes are parsed correctly 
    age int
    address PersonAddress optional
    isEmployed bool
    department Employee_department
}

schema {
    message str
    number int
    person Person optional
}